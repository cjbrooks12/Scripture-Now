<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/color_background"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin" >
       
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="vertical" >
	   	<HorizontalScrollView 
	   	    android:id="@+id/a"
	   	    android:layout_alignParentLeft="true"
	   	    android:layout_alignParentTop="true"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content">
	        <LinearLayout 
	            android:layout_width="wrap_content"
	        	android:layout_height="wrap_content">
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_accept" />
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_back" />
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_call" />
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_cancel" />
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_collapse" />
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_copy" />
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_cut" />
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_discard" />
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_edit" />
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_expand" />
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_forward" />
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_go_to_today" />
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_make_available_offline" />
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_new" />
				<ImageView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_new_event" />
				<ImageView					
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_next_item" />
				<ImageView					
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_overflow" />
				<ImageView					
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_paste" />
				<ImageView					
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_previous" />
				<ImageView					
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_refresh" />
				<ImageView					
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_remove" />
				<ImageView					
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_save" />
				<ImageView					
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_search" />
				<ImageView					
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_select" />
				<ImageView					
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_share" />
				<ImageView					
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="?attr/ic_action_undo" />
		</LinearLayout>
	</HorizontalScrollView>
    <TextView
		android:id="@+id/b"
		android:layout_below="@id/a"
		android:layout_width="match_parent"
		android:layout_height="64dp"
		android:background="?attr/bg_card"
		android:gravity="center"
		android:text="TextView" />
    <DatePicker
		android:id="@+id/c"
		android:layout_below="@id/b"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="?attr/bg_card"
		android:layout_centerHorizontal="true"
		android:calendarViewShown="false" />
    <TimePicker
		android:id="@+id/d"
		android:layout_below="@id/c"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="?attr/bg_card"
		android:layout_centerHorizontal="true" />
    <Button
        android:id="@+id/e"
		android:layout_below="@id/d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:text="Button 1" />
    <ToggleButton
        android:id="@+id/f"
		android:layout_below="@id/d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Button 2" />
    <Button
        android:id="@+id/g"
        android:background="?attr/bg_transparent"
		android:layout_below="@id/d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:text="Button 3" />
    <RatingBar
        android:id="@+id/h"
		android:layout_below="@id/g"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true" />
    <CheckBox
        android:id="@+id/i"
		android:layout_below="@id/h"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:text="CheckBox 1" />
    <CheckBox
        android:id="@+id/j"
		android:layout_below="@id/i"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:text="CheckBox 2" />
    <CheckBox
        android:id="@+id/k"
		android:layout_below="@id/j"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:text="CheckBox 3" />
    
    <RadioGroup
        android:id="@+id/l"
		android:layout_below="@id/h"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true" >
        <RadioButton
            android:id="@+id/radio0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Radio Button1" />
        <RadioButton
            android:id="@+id/radio1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Radio Button2" />
        <RadioButton
            android:id="@+id/radio2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Radio Button3" />
    </RadioGroup>
    <ProgressBar
        android:id="@+id/m"
        android:layout_below="@id/l"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true" />
    <ProgressBar
        android:id="@+id/n"
        android:layout_below="@id/m"
        style="?android:attr/progressBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true" />
    <ProgressBar
        android:id="@+id/o"
        android:layout_below="@id/m"
        style="?android:attr/progressBarStyleInverse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true" />
    <ProgressBar
        android:id="@+id/p"
        android:layout_below="@id/m"
        style="?android:attr/progressBarStyleLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true" />
    <SeekBar
        android:id="@+id/q"
        android:layout_below="@id/p"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true" />
    <Spinner
        android:id="@+id/r"
        android:layout_below="@id/q"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true" />
    <EditText
        android:id="@+id/s"
        android:layout_below="@id/r"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10" 
        android:text="Lorem ipsum dolor sit amet, consectetur adipiscing elit. Duis pulvinar commodo mi sed hendrerit. Sed vitae vulputate elit, a vestibulum nibh. Mauris ultrices nisl dui, eget sollicitudin nisl gravida sit amet. Nullam eu nibh in.">
    </EditText>
    
	</RelativeLayout>
</ScrollView>

