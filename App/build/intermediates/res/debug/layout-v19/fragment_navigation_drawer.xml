<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="?attr/color_background"
	android:orientation="vertical">
	<View
		android:id="@+id/statusBar"
		android:layout_width="wrap_content"
		android:layout_height="25dp"/>
	<com.nirhart.parallaxscroll.views.ParallaxExpandableListView
		android:background="?attr/color_background"
		android:id="@+id/navListView"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:divider="@android:color/transparent"
		android:dividerHeight="0dp"
		android:groupIndicator="@null"
		app:parallax_factor="1.5" />
</LinearLayout><!-- From: file:/C:/Users/Casey/Documents/android/scripturememory/App/src/main/res/layout-v19/fragment_navigation_drawer.xml -->